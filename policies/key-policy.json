{
  "Version" : "2012-10-17",
  "Statement" : [ {
    "Sid" : "AllowRootAccess",
    "Effect" : "Allow",
    "Principal" : {
      "AWS" : "arn:aws:iam::429128462098:root"
    },
    "Action" : "kms:*",
    "Resource" : "*"
  }, {
    "Sid" : "AllowRekognitionAccess",
    "Effect" : "Allow",
    "Principal" : {
      "Service" : "rekognition.amazonaws.com"
    },
    "Action" : [ "kms:Decrypt", "kms:GenerateDataKey" ],
    "Resource" : "*",
    "Condition" : {
      "StringEquals" : {
        "aws:SourceAccount" : "429128462098"
      }
    }
  } ]
}	default
