{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowRekognitionAccess",
      "Effect": "Allow",
      "Principal": {
        "Service": "rekognition.amazonaws.com"
      },
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::noorshare-user-uploads/*",
      "Condition": {
        "StringEquals": {
          "AWS:SourceAccount": "429128462098"
        }
      }
    }
  ]
}
